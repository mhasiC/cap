#include<stdio.h>
void main()
{
    int a[10],i,j,m,n,c=0;
    printf("Enter array elements : ");
    for(i=0;i<10;i++)
      scanf("%d",&a[i]);
    printf("Enter item to remove : ");
      scanf("%d",&m);
    for (i=0;i<10-c;i++)
      {
        if (a[i]==m)
        {
          c++;
          for(j=i;j<10-c;j++)
            a[j]=a[j+1];
          i--;
        }
      }
    for (i=0;i<10-c;i++)
      printf("%d",a[i]);
    printf("\nOccurance of %d = %d\n",m,c);
    if (c>4)
      printf("Array does not include specified location"); //5th position
     else
     {
        printf("Enter item to add : ");
        scanf("%d",&n);
        for (i=10-c;i>4;i++)
          a[i]=a[i-1];
         a[4]=n;
         c--;
         printf("final array : ");
         for(i=0;i<10-c;i--)
          printf("%d",a[i]);
     }
}
